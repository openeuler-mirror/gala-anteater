{
  "name": "AppSliDetector",
  "enable": false,
  "keywords": [
    "app"
  ],
  "root_cause_num": 20,
  "detector": "vae",
  "template": "app",
  "kpis": [
    {
      "metric": "gala_gopher_sli_rtt_nsec",
      "entity_name": "sli",
      "enable": true,
      "atrend": "rise",
      "params": {
        "method": "max",
        "look_back": 10,
        "obs_size": 25,
        "outlier_ratio_th": 0.5,
        "smooth_params": {
          "method": "conv_smooth",
          "box_pts": 3
        }
      }
    },
    {
      "metric": "gala_gopher_sli_tps",
      "entity_name": "sli",
      "enable": true,
      "atrend": "fall",
      "params": {
        "method": "min",
        "look_back": 10,
        "obs_size": 25,
        "outlier_ratio_th": 0.3,
        "smooth_params": {
          "method": "conv_smooth",
          "box_pts": 3
        }
      }
    }
  ],
  "model_config": {
    "name": "vae_model",
    "params": {
      "th": 0.5,
      "max_error_rate": 0.7,
      "min_retrain_hours": 24,
      "min_predict_minutes": 20,
      "norm": {},
      "vae": {
        "hidden_sizes": [
          25,
          10,
          5
        ],
        "latent_size": 5,
        "dropout_rate": 0.25,
        "batch_size": 1024,
        "num_epochs": 30,
        "learning_rate": 0.001,
        "k": 120,
        "step_size": 30,
        "num_eval_samples": 10
      },
      "threshold": {},
      "dy_threshold": {
        "min_percent": -1
      },
      "calibrate": {},
      "error_calibrate": {
        "fixed_threshold": false,
        "n": 3,
        "quantile": 0.99,
        "alpha": 0.002
      }
    }
  },
  "features": [
    {
      "metric": "gala_gopher_block_count_latency_req"
    },
    {
      "metric": "gala_gopher_block_latency_req_jitter",
