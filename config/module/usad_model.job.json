      "entity_name": "l7",
      "atrend": "fall",
      "enable": true
    },
    {
      "metric": "gala_gopher_l7_throughput_resp",
      "entity_name": "l7",
      "atrend": "fall",
      "enable": true
    },
    {
      "metric": "gala_gopher_l7_err_ratio",
