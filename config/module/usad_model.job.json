      "atrend": "fall",
      "enable": true
    },
    {
      "metric": "gala_gopher_l7_err_count",
      "entity_name": "l7",
      "atrend": "fall",
      "enable": true
    },
    {
      "metric": "gala_gopher_proc_flow_perf_tx_delay",
