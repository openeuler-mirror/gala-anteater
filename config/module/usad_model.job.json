{
  "name": "AppSliDetector",
  "enable": true,
  "job_type": "anomaly_detection",
  "keywords": [
    "app"
  ],
  "root_cause_num": 20,
  "detector": "usad",
  "template": "net",
  "kpis": [
    {
      "metric": "gala_gopher_sli_rtt_nsec",
      "entity_name": "sli",
      "atrend": "rise",
      "enable": true
    },
    {
      "metric": "gala_gopher_sli_tps",
      "entity_name": "sli",
      "atrend": "fall",
      "enable": true
    },
    {
      "metric": "gala_gopher_l7_latency_avg",
      "entity_name": "l7",
      "atrend": "fall",
      "enable": true
    },
    {
      "metric": "gala_gopher_l7_latency_sum",
      "entity_name": "l7",
      "atrend": "fall",
      "enable": true
    },
